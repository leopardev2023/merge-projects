{
  "rewrites": [
    { "source": "/app/", "destination": "https://outdefine-frontend.vercel.app/" },
    {
      "source": "/app/:match*",
      "destination": "https://outdefine-frontend.vercel.app/:match*"
    },
    { "source": "/", "destination": "https://www.outdefine.com/" },
    {
      "source": "/:match*",
      "destination": "https://www.outdefine.com/:match*"
    }
  ]
}
